{
  "name": "rectospec",
  "version": "0.1.0",
  "description": "Chrome Recorder to Gherkin to Playwright - AI-powered test automation tool",
  "keywords": [
    "playwright",
    "gherkin",
    "bdd",
    "chrome-recorder",
    "testing",
    "ai",
    "test-automation"
  ],
  "author": "Dev-kenta",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "CONTRIBUTING.md"
  ],
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Dev-kenta/rectospec.git"
  },
  "homepage": "https://github.com/Dev-kenta/rectospec#readme",
  "bugs": {
    "url": "https://github.com/Dev-kenta/rectospec/issues"
  },
  "bin": {
    "rectospec": "./dist/index.js"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "type": "module",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build && npm test -- --run"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^3.0.2",
    "@ai-sdk/google": "^0.0.52",
    "ai": "^3.4.0",
    "chalk": "^5.3.0",
    "commander": "^12.1.0",
    "dotenv": "^16.4.5",
    "express": "^5.2.1",
    "inquirer": "^12.11.1",
    "ora": "^8.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^22.9.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "@vitest/coverage-v8": "^2.1.5",
    "eslint": "^9.15.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.5"
  }
}
